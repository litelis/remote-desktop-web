# Puerto del servidor (cambiar si está en uso)
PORT=8443

# Contraseña de acceso (¡CAMBIAR EN PRODUCCIÓN!)
ADMIN_PASSWORD=admin123

# Clave secreta para JWT (generar una larga y aleatoria)
JWT_SECRET=super-secret-key-change-in-production-minimo-32-caracteres

# Entorno: development o production
NODE_ENV=development

# URL del frontend (para CORS)
CORS_ORIGIN=http://localhost:3000

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=5